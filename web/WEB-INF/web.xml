<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>RouteFilter</filter-name>
        <filter-class>fu.ex.Filter.RouteFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>RouteFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>ViewList_index</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewList_index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewList_user</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewList_user</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewList_found</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewList_found</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewList_dashboard</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewList_dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateItem</servlet-name>
        <servlet-class>fu.ex.Servlet.CreateItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewList_lost</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewList_lost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>fu.ex.Servlet.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewHostelInfo_admin</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewHostelInfo_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>fu.ex.Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateProfileServlet</servlet-name>
        <servlet-class>fu.ex.Servlet.updateProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewList_index</servlet-name>
        <url-pattern>/ViewList_index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewList_user</servlet-name>
        <url-pattern>/ViewList_user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewList_found</servlet-name>
        <url-pattern>/ViewList_found</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateItem</servlet-name>
        <url-pattern>/CreateItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewList_lost</servlet-name>
        <url-pattern>/ViewList_lost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewList_dashboard</servlet-name>
        <url-pattern>/ViewList_dashboard</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>ViewList_index</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateProfileServlet</servlet-name>
        <url-pattern>/updateProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>YourPost</servlet-name>
        <servlet-class>fu.ex.Servlet.YourPost</servlet-class>
    </servlet>
