        <servlet-name>YourPost</servlet-name>
        <url-pattern>/YourPost</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>ViewList_report</servlet-name>
        <servlet-class>fu.ex.Servlet.ViewList_report</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewList_report</servlet-name>
        <url-pattern>/ViewList_report</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
